{
  "rust-analyzer.linkedProjects": ["apps/server/wasm-src/Cargo.toml"],
  "rust-analyzer.workspace.symbol.search.scope": "workspace_and_dependencies",
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.cargo.workspaceSymbol.excludeDirs": [
    "apps/mobile",
    "apps/dashboard",
    "node_modules",
    "target"
  ],
  "rust-analyzer.files.excludeDirs": ["apps/mobile", "apps/dashboard", "node_modules"]
}
