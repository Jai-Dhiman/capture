    {
      "name": "GraphQL",
      "item": [
        {
          "name": "GraphQL Query",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query { profile(id: \\\"user-id\\\") { id username displayName bio } }\",\n  \"variables\": {}\n}"
            }
          }
        },
        {
          "name": "Get Post",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query GetPost($id: ID!) { post(id: $id) { id caption createdAt user { id username displayName } media { id url } likeCount commentCount } }\",\n  \"variables\": {\"id\": \"post-id\"}\n}"
            }
          }
        },
        {
          "name": "Get Discovery Feed",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query DiscoverFeed($limit: Int, $cursor: String) { discoverFeed(limit: $limit, cursor: $cursor) { posts { id caption user { username } media { url } } nextCursor hasMore } }\",\n  \"variables\": {\"limit\": 20}\n}"
            }
          }
        },
        {
          "name": "Get Following Feed",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query FollowingFeed($limit: Int, $cursor: String) { followingFeed(limit: $limit, cursor: $cursor) { posts { id caption user { username } } nextCursor hasMore } }\",\n  \"variables\": {\"limit\": 20}\n}"
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query GetProfile($id: ID!) { profile(id: $id) { id username displayName bio profileImageUrl followerCount followingCount postCount isFollowing isPrivate } }\",\n  \"variables\": {\"id\": \"user-id\"}\n}"
            }
          }
        },
        {
          "name": "Get Comments",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/graphql",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"query GetComments($postId: ID!, $limit: Int, $offset: Int) { comments(postId: $postId, limit: $limit, offset: $offset) { id content createdAt user { id username } replyCount } }\",\n  \"variables\": {\"postId\": \"post-id\", \"limit\": 20, \"offset\": 0}\n}"
            }
          }
        },
        {
          "name": "Search Users",
          "request": {
            "method": "POST",
